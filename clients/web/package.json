{
  "name": "isophp",
  "displayName": "isophp",
  "version": "1.0.0",
  "author": "Pharaoh Tools",
  "license": "none",
  "description": "The ISO PHP Framework Web Client Applicatiion",
  "main": "main.js",
  "scripts": {
    "build": "node fs > /dev/null && ( browserify app --standalone=isophp | uglifyjs > uniter_bundle/bundle.js)",
    "build-webpack-one": "browserify index > dist/bundle.js",
    "build-webpack-two": "node_modules/.bin/webpack --devtool source-map"
  },
  "repository": "https://anon:any@source.internal.pharaohtools.com/git/public/isophp",
  "keywords": [
    "PharaohTools",
    "uniter",
    "php",
    "browserify",
    "fs"
  ],
  "devDependencies": {
    "async": "^2.1.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "browserify": "^10.2.0",
    "globby": "^2.0.0",
    "jquery": "^2.1.4",
    "json-loader": "^0.5.1",
    "node-libs-browser": "^0.5.0",
    "phpify": "^4.0.0",
    "phpjs": "^1.3.2",
    "socket.io": "1.2.0",
    "source-map-loader": "^0.2.3",
    "transform-loader": "^0.2.4",
    "uglify-js": "^2.4.23",
    "uglifyify": "^3.0.1",
    "uniter": "^1.0",
    "uuid": "^3.0.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "browserify": {
    "transform": [
      "uglifyify"
    ]
  },
  "phpify": {
    "phpToJS": {
      "sync": true,
      "include": [
        "app/*.php",
        "app/**/*.php"
      ]
    }
  },
  "dependencies": {
    "webpack-cli": "^3.0.6"
  }
}
